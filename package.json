{
  "name": "browse",
  "description": "Adds commands that let you quickly browse Atom-related folders or reveal files you're working on",
  "version": "1.1.5",
  "license": "MIT",
  "author": {
    "name": "Jan T. Sott",
    "url": "http://github.com/idleberg"
  },
  "keywords": [
    "developer",
    "browse",
    "explorer",
    "file manager",
    "finder"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/idleberg/atom-browse.git"
  },
  "main": "./lib/browse",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "activationCommands": {
    "atom-workspace": [
      "browse:configuration-folder",
      "browse:reveal-all-open-files",
      "browse:reveal-file",
      "browse:project-folders",
      "browse:packages-folder"
    ]
  },
  "devDependencies": {
    "gulp": "^3.9.1",
    "gulp-coffeelint": "^0.6.0",
    "gulp-debug": "^2.1.2",
    "gulp-jshint": "^2.0.1",
    "gulp-jsonlint": "^1.1.2",
    "gulp-lesshint": "^2.0.0",
    "jshint": "^2.9.2"
  },
  "scripts": {
    "test": "gulp lint"
  }
}
