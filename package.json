{
  "name": "browse",
  "main": "./lib/browse",
  "version": "2.6.2",
  "description": "Adds commands that let you quickly browse Atom-related folders or reveal files you're working on",
  "scripts": {
    "build": "webpack --mode production",
    "clean": "rimraf ./lib",
    "dev": "npm run start",
    "lint": "tslint --project ./",
    "lint:fix": "tslint --fix --project ./",
    "prepublishOnly": "npm run clean && npm run build",
    "start": "webpack --mode none --watch",
    "test": "npm run lint"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/atom-browse"
  },
  "homepage": "https://atom.io/packages/browse",
  "bugs": {
    "url": "https://github.com/idleberg/atom-browse/issues"
  },
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "activationCommands": {
    "atom-workspace": [
      "browse:.apm-folder",
      "browse:app-data-folder",
      "browse:application-folder",
      "browse:configuration-folder",
      "browse:packages-folder",
      "browse:project-dependencies",
      "browse:project-folders",
      "browse:resources-folders",
      "browse:reveal-file",
      "browse:reveal-open-files"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@types/atom": "^1.40.4",
    "@types/node": "^14.0.24",
    "find-up": "^4.1.0",
    "husky": "^4.2.5",
    "rimraf": "^3.0.2",
    "ts-loader": "^8.0.1",
    "tslint": "^6.1.2",
    "typescript": "^3.9.7",
    "util": "^0.12.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
