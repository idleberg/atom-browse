{
  "name": "browse",
  "version": "3.9.0",
  "description": "Adds commands that let you quickly browse Atom-related folders or reveal files you're working on",
  "scripts": {
    "build": "tsup",
    "dev": "npm run build -- --watch",
    "lint:json": "eslint ./keymaps/*.json menus/*.json --ignore-path .gitignore",
    "lint:md": "remark . --quiet --frail --ignore-path .gitignore",
    "lint:ts": "eslint ./src --ignore-path .gitignore",
    "lint": "npm-run-all lint:*",
    "prepublishOnly": "npm run build",
    "start": "npm run dev",
    "test": "npm run lint",
    "prepare": "husky"
  },
  "main": "./lib/index",
  "typings": "./types",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/atom-browse"
  },
  "homepage": "https://atom.io/packages/browse",
  "bugs": {
    "url": "https://github.com/idleberg/atom-browse/issues"
  },
  "license": "MIT",
  "engines": {
    "atom": ">=1.52.0 <2.0.0"
  },
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "providedServices": {
    "browse": {
      "description": "Opens files/folders in your file manager",
      "versions": {
        "1.0.0": "provideBrowse"
      }
    }
  },
  "devDependencies": {
    "@atxm/developer-console": "^0.5.0",
    "@stylelint/remark-preset": "^3.0.0",
    "@types/atom": "^1.40.4",
    "@types/electron": "^1.6.10",
    "@types/node": "^16.11.26",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "atom-package-deps": "^8.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.13.0",
    "eslint-plugin-json": "^3.1.0",
    "find-up": "^5.0.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "log-symbols": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "remark-cli": "^10.0.0",
    "stylelint": "^14.6.1",
    "tslib": "^2.3.1",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2"
  },
  "remarkConfig": {
    "plugins": [
      "@stylelint/remark-preset"
    ]
  },
  "lint-staged": {
    "*.md": "prettier --write",
    "*.(json|ts)": "eslint --cache --fix"
  }
}
