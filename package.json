{
  "name": "browse",
  "description": "Adds commands that let you quickly browse Atom-related folders or reveal files you're working on",
  "version": "1.3.4",
  "license": "MIT",
  "author": {
    "name": "Jan T. Sott",
    "url": "http://github.com/idleberg"
  },
  "keywords": [
    "developer",
    "browse",
    "explorer",
    "file manager",
    "finder"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/idleberg/atom-browse.git"
  },
  "main": "./lib/browse",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "activationCommands": {
    "atom-workspace": [
      "browse:configuration-folder",
      "browse:reveal-all-open-files",
      "browse:reveal-file",
      "browse:project-folders",
      "browse:packages-folder"
    ]
  },
  "dependencies": {
    "shell": "^0.3.2"
  },
  "devDependencies": {
    "coffeelint-stylish": "^0.1.2",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-coffeelint": "^0.6.0",
    "gulp-debug": "^3.0.0",
    "gulp-jshint": "^2.0.4",
    "gulp-jsonlint": "^1.1.2",
    "gulp-lesshint": "^2.1.0",
    "jshint": "^2.9.4"
  },
  "scripts": {
    "test": "gulp lint"
  }
}
