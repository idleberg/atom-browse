{
  "name": "browse",
  "version": "3.6.1",
  "description": "Adds commands that let you quickly browse Atom-related folders or reveal files you're working on",
  "scripts": {
    "build": "node ./tools/build.mjs",
    "clean": "rimraf ./lib",
    "dev": "npm run start",
    "lint:json": "jsonlint ./keymaps/*.json menus/*.json --quiet",
    "lint:md": "remark . --quiet --frail --ignore-path .gitignore",
    "lint:ts": "eslint ./src --ignore-path .gitignore",
    "lint": "npm-run-all lint:*",
    "prepublishOnly": "npm run clean && npm run build",
    "start": "cross-env ESBUILD_WATCH=true node ./tools/build.mjs",
    "test": "npm run lint"
  },
  "main": "./lib/browse",
  "typings": "./types",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/atom-browse"
  },
  "homepage": "https://atom.io/packages/browse",
  "bugs": {
    "url": "https://github.com/idleberg/atom-browse/issues"
  },
  "license": "MIT",
  "engines": {
    "atom": ">=1.52.0 <2.0.0"
  },
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "providedServices": {
    "browse": {
      "description": "Opens files/folders in your file manager",
      "versions": {
        "1.0.0": "provideBrowse"
      }
    }
  },
  "devDependencies": {
    "@atxm/developer-console": "^0.5.0",
    "@stylelint/remark-preset": "^2.0.0",
    "@types/atom": "^1.40.4",
    "@types/node": "^15.3.1",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "atom-package-deps": "^7.0.3",
    "cross-env": "^7.0.3",
    "esbuild": "^0.12.1",
    "eslint": "^7.26.0",
    "eslint-plugin-json": "^3.0.0",
    "find-up": "^5.0.0",
    "husky": ">=4.3 <5",
    "jsonlint": "^1.6.3",
    "lint-staged": "^11.0.0",
    "log-symbols": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.0",
    "remark-cli": "^9.0.0",
    "rimraf": "^3.0.2",
    "stylelint": "^13.13.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3"
  },
  "remarkConfig": {
    "plugins": [
      "@stylelint/remark-preset"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.json": "jsonlint --quiet",
    "*.md": "prettier --write",
    "*.ts": "eslint --cache --fix"
  }
}
