{
  "name": "browse",
  "version": "3.1.2",
  "description": "Adds commands that let you quickly browse Atom-related folders or reveal files you're working on",
  "scripts": {
    "build": "rollup -c",
    "clean": "rimraf ./lib",
    "dev": "npm run start",
    "lint": "eslint --fix ./src",
    "prepublishOnly": "npm run clean && npm run build",
    "start": "rollup -w -c",
    "test": "npm run lint"
  },
  "main": "./lib/browse",
  "typings": "./types",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/atom-browse"
  },
  "homepage": "https://atom.io/packages/browse",
  "bugs": {
    "url": "https://github.com/idleberg/atom-browse/issues"
  },
  "license": "MIT",
  "engines": {
    "atom": ">=1.28.0 <2.0.0"
  },
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "providedServices": {
    "browse": {
      "description": "Opens files/folders in your file manager",
      "versions": {
        "1.0.0": "provideBrowse"
      }
    }
  },
  "devDependencies": {
    "@atxm/developer-console": "^0.3.0",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@types/atom": "^1.40.4",
    "@types/electron": "^1.6.10",
    "@types/node": "^14.11.10",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "atom-package-deps": "^7.0.2",
    "eslint": "^7.11.0",
    "eslint-plugin-json": "^2.1.2",
    "find-up": "^5.0.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.32.0",
    "rollup-plugin-filesize": "^9.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": "npm run lint"
  }
}
