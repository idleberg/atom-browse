{
	"name": "browse",
	"version": "3.9.2",
	"description": "Adds commands that let you quickly browse editor-related folders or reveal files you're working on",
	"scripts": {
		"build": "tsup",
		"dev": "npm run build -- --watch",
		"lint": "concurrently --prefix-colors blue,green,magenta npm:lint:*",
		"lint:biome": "biome check",
		"lint:e18e": "e18e-cli analyze",
		"prepublishOnly": "npm run build",
		"start": "npm run dev",
		"test": "npm run lint",
		"prepare": "lefthook install"
	},
	"main": "./lib/index",
	"typings": "./types",
	"keywords": [],
	"repository": {
		"type": "git",
		"url": "https://github.com/idleberg/atom-browse"
	},
	"homepage": "https://web.pulsar-edit.dev/packages/browse",
	"bugs": {
		"url": "https://github.com/idleberg/atom-browse/issues"
	},
	"license": "MIT",
	"engines": {
		"atom": ">=1.52.0 <2.0.0"
	},
	"activationHooks": [
		"core:loaded-shell-environment"
	],
	"providedServices": {
		"browse": {
			"description": "Opens files/folders in your file manager",
			"versions": {
				"2.0.0": "provideBrowse"
			}
		}
	},
	"devDependencies": {
		"@atxm/developer-console": "^0.6.1",
		"@commitlint/cli": "^20.4.1",
		"@commitlint/config-conventional": "^20.4.1",
		"@idleberg/configs": "^0.4.2",
		"@stylelint/remark-preset": "^5.1.1",
		"@types/atom": "^1.40.18",
		"@types/electron": "^1.6.12",
		"@types/node": "^14.18.63",
		"atom-package-deps": "^8.0.0",
		"concurrently": "^9.2.1",
		"cross-env": "^7.0.3",
		"find-up": "^5.0.0",
		"log-symbols": "^5.1.0",
		"tslib": "^2.8.1",
		"tsup": "^8.5.1",
		"typescript": "^5.9.3"
	}
}
