{
  "name": "browse",
  "version": "3.3.2",
  "description": "Adds commands that let you quickly browse Atom-related folders or reveal files you're working on",
  "scripts": {
    "build": "rollup -c",
    "clean": "rimraf ./lib",
    "dev": "npm run start",
    "lint:json": "jsonlint ./keymaps/*.json menus/*.json --quiet",
    "lint:md": "remark . --quiet --frail --ignore-path .gitignore",
    "lint:ts": "eslint ./src --ignore-path .gitignore",
    "lint": "npm-run-all lint:*",
    "prepublishOnly": "npm run clean && npm run build",
    "start": "rollup -w -c",
    "test": "npm run lint"
  },
  "main": "./lib/browse",
  "typings": "./types",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/atom-browse"
  },
  "homepage": "https://atom.io/packages/browse",
  "bugs": {
    "url": "https://github.com/idleberg/atom-browse/issues"
  },
  "license": "MIT",
  "engines": {
    "atom": ">=1.28.0 <2.0.0"
  },
  "activationHooks": [
    "core:loaded-shell-environment"
  ],
  "providedServices": {
    "browse": {
      "description": "Opens files/folders in your file manager",
      "versions": {
        "1.0.0": "provideBrowse"
      }
    }
  },
  "devDependencies": {
    "@atxm/developer-console": "^0.5.0",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-typescript": "^8.1.0",
    "@types/atom": "^1.40.4",
    "@types/electron": "^1.6.10",
    "@types/node": "^14.14.39",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "atom-package-deps": "^7.0.3",
    "eslint": "^7.16.0",
    "eslint-plugin-json": "^2.1.2",
    "find-up": "^5.0.0",
    "husky": ">=4.3 <5",
    "jsonlint": "^1.6.3",
    "lint-staged": "^10.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "remark-cli": "^9.0.0",
    "remark-preset-lint-recommended": "^5.0.0",
    "remark-preset-prettier": "^0.4.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.35.1",
    "rollup-plugin-filesize": "^9.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3"
  },
  "remarkConfig": {
    "plugins": [
      "preset-lint-recommended",
      "remark-preset-prettier"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.json": "jsonlint --quiet",
    "*.md": "prettier --write",
    "*.ts": "eslint --cache --fix"
  }
}
