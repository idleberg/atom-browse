{
  "name": "browse",
  "description": "Adds commands that let you quickly browse Atom-related folders or reveal files you're working on",
  "version": "1.10.8",
  "license": "MIT",
  "author": {
    "name": "Jan T. Sott",
    "url": "http://github.com/idleberg"
  },
  "keywords": [
    "developer",
    "browse",
    "explorer",
    "file manager",
    "finder"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/atom-browse"
  },
  "main": "./lib/browse",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "activationCommands": {
    "atom-workspace": [
      "browse:.apm-folder",
      "browse:application-folder",
      "browse:configuration-folder",
      "browse:packages-folder",
      "browse:project-folders",
      "browse:reveal-all-open-files",
      "browse:reveal-file"
    ],
    ".tree-view": "browse:reveal-file-from-tree-view"
  },
  "dependencies": {
    "getmac": "^1.4.6"
  },
  "devDependencies": {
    "coffeelint-stylish": "^0.1.2",
    "gulp": "^4.0.2",
    "gulp-coffeelint": "^0.6.0",
    "gulp-debug": "^4.0.0",
    "gulp-jsonlint": "^1.3.0",
    "husky": "^2.4.0"
  },
  "scripts": {
    "test": "gulp lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test"
    }
  }
}
