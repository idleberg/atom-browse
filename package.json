{
  "name": "browse",
  "version": "2.7.0",
  "description": "Adds commands that let you quickly browse Atom-related folders or reveal files you're working on",
  "scripts": {
    "build": "rollup -c",
    "clean": "rimraf ./lib",
    "dev": "npm run start",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "prepublishOnly": "npm run clean && npm run build",
    "start": "rollup -w -c",
    "test": "npm run lint"
  },
  "main": "./lib/browse",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/atom-browse"
  },
  "homepage": "https://atom.io/packages/browse",
  "bugs": {
    "url": "https://github.com/idleberg/atom-browse/issues"
  },
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "activationCommands": {
    "atom-workspace": [
      "browse:.apm-folder",
      "browse:app-data-folder",
      "browse:application-folder",
      "browse:configuration-folder",
      "browse:packages-folder",
      "browse:project-dependencies",
      "browse:project-folders",
      "browse:resources-folders",
      "browse:reveal-file",
      "browse:reveal-open-files"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "@types/atom": "^1.40.4",
    "@types/node": "^14.0.24",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "eslint": "^7.5.0",
    "find-up": "^4.1.0",
    "husky": "^4.2.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.22.1",
    "rollup-plugin-terser": "^6.1.0",
    "typescript": "^3.9.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
