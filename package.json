{
	"name": "browse",
	"version": "3.9.2",
	"description": "Adds commands that let you quickly browse editor-related folders or reveal files you're working on",
	"scripts": {
		"build": "tsup",
		"dev": "npm run build -- --watch",
		"lint:md": "remark . --quiet --frail --ignore-path .gitignore",
		"lint:ts": "eslint ./src --ignore-pattern .gitignore --no-warn-ignored",
		"lint": "concurrently --prefix '{name}' -c 'blue,green' 'npm:lint:*'",
		"prepublishOnly": "npm run build",
		"start": "npm run dev",
		"test": "npm run lint",
		"prepare": "husky"
	},
	"main": "./lib/index",
	"typings": "./types",
	"keywords": [],
	"repository": {
		"type": "git",
		"url": "https://github.com/idleberg/atom-browse"
	},
	"homepage": "https://web.pulsar-edit.dev/packages/browse",
	"bugs": {
		"url": "https://github.com/idleberg/atom-browse/issues"
	},
	"license": "MIT",
	"engines": {
		"atom": ">=1.52.0 <2.0.0"
	},
	"activationHooks": [
		"core:loaded-shell-environment"
	],
	"providedServices": {
		"browse": {
			"description": "Opens files/folders in your file manager",
			"versions": {
				"1.0.0": "provideBrowse"
			}
		}
	},
	"devDependencies": {
		"@atxm/developer-console": "^0.6.0",
		"@commitlint/cli": "^20.4.1",
		"@commitlint/config-conventional": "^20.4.1",
		"@idleberg/configs": "^0.4.2",
		"@stylelint/remark-preset": "^5",
		"@types/atom": "^1.40.4",
		"@types/electron": "^1.6.10",
		"@types/node": "^14",
		"atom-package-deps": "^8.0.0",
		"concurrently": "^9.0.1",
		"cross-env": "^7.0.3",
		"eslint": "^9.11.0",
		"eslint-plugin-jsonc": "^2.16.0",
		"eslint-plugin-unicorn": "^55.0.0",
		"find-up": "^5.0.0",
		"husky": "^9.1.6",
		"lint-staged": "^15.2.10",
		"log-symbols": "^5.0.0",
		"prettier": "^3",
		"remark-cli": "^12",
		"stylelint": "^16",
		"tslib": "^2.3.1",
		"tsup": "^8.3.0",
		"typescript": "^5.6.2",
		"typescript-eslint": "^8.6.0"
	},
	"remarkConfig": {
		"plugins": [
			"@stylelint/remark-preset"
		]
	},
	"lint-staged": {
		"*.(ts)": [
			"eslint --cache --fix",
			"prettier --write"
		],
		"*.{md,yml}": "prettier --write"
	}
}
